generator client {
  provider = "prisma-client-js"
  output   = env("BUDGET_PAL_PRISMA_CLIENT")

}

datasource db {
  provider = "postgresql"
  url      = env("BUDGET_PAL_DATABASE_URL")
}

model User {
  id        String   @id @unique 
  email     String   @unique
  createdAt DateTime @default(now()) @map("created_at")
  updatedAt DateTime @updatedAt @map("updated_at")
  imgUrl    String?
  firtsName      String @map("firts_name")
  lastName       String  @map("last_name")
}